;af_zg_2d (TopSpin 2.0)

; ACF's 2D zg experiment 

;parameters:
;ns : 8 * n
;d1 : recycle delay
;pl1 : RF power level
;p1 : 90 degree pulse
;$CLASS=Solids
;$DIM=2D
;$TYPE=direct excitation
;$SUBTYPE=simple 1D

1 ze
2 10m 

  d1

  10u pl1:f1
  (p1 ph1):f1
  
  go=2 ph31

  10m mc #0 to 2 F1QF(id0) 
exit

ph1=0 2 2 0 1 3 3 1
ph31=0 2 2 0 1 3 3 1
