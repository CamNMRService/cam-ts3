;zg-slice-selective

#include <Avance.incl>
#include <Grad.incl>

"acqt0=-p1*2/3.1416"
;set cnst20 for calculation purposes
"cnst20=cnst19"

1 ze
2 	30m
	d1
	3u UNBLKGRAD
	3u gron1
	(p11:sp1 ph1):f1
	3u groff
	3u BLKGRAD
	go=2 ph31
	30m mc #0 to 2 F0(zd)
exit

ph1=0 2 2 0 1 3 3 1
ph31=0 2 2 0 1 3 3 1

;d1: relaxation delay
;cnst19: slice width in Hz
;sp1: f1 channel - shaped pulse power level for band-selective excitation
;spnam2: shaped pulse for selective excitation

;BEGIN WAVEMAKER
;sp1:wvm:G4(cnst19 Hz)
;END WAVEMAKER





		
